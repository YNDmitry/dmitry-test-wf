{
  "name": "dmitry-test-wf",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wf-publish --env=dev --dev-port=5173",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish:global": "wf-publish --env=prod",
    "publish:pages": "wf-publish --env=prod --by-page",
    "lint": "eslint src/**/**/*.{js,ts}",
    "format": "prettier --write src/**/**/*.{js,ts}",
    "lint:fix": "eslint src/scripts/**/**/*.{js,ts} --fix"
  },
  "devDependencies": {
    "typescript": "~5.6.2",
    "vite": "^6.0.5"
  },
  "dependencies": {
    "@types/jquery": "^3.5.32",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "@yndmitry/vite-plugin-pages": "^0.1.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.4.2"
  },
  "config": {
    "ghUserName": "yndmitry",
    "repositoryName": "dmitry-test-wf",
    "packageManager": "bun"
  }
